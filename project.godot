config_version=4

[application]

config/name="Kojum: Combat Ball"
run/main_scene="res://ui/branding/Branding.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="kojum"
boot_splash/image="res://assets/textures/ui/splash.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://iconx256.png"
config/windows_native_icon="res://kojum_icon2.ico"

[autoload]

MenuSwitcher="*res://global/Menu_Switcher.tscn"
Render="*res://global/Render.gd"
Global="*res://global/Global.gd"
Asset="*res://global/Asset.gd"
MenuMusic="*res://global/Music.tscn"

[display]

window/size/width=512
window/size/height=288
window/size/test_width=1024
window/size/test_height=576
window/stretch/aspect="keep"

[importer_defaults]

texture={
